{
  "raw_dataset_path": "./data/raw/movies_metadata.parquet",
  "clean_dataset_path": "./data/cleaned/movies_metadata.parquet",
  "plot_directory_path": "plots",
  "attributes": {
    "title": {
      "type": "nominal"
    },
    "vote_average": {
      "type": "real"
    },
    "vote_count": {
      "type": "real"
    },
    "status": {
      "type": "nominal"
    },
    "release_date": {
      "type": "nominal"
    },
    "revenue": {
      "type": "real"
    },
    "runtime": {
      "type": "real"
    },
    "adult": {
      "type": "nominal"
    },
    "budget": {
      "type": "real"
    },
    "popularity": {
      "type": "real"
    },
    "genres": {
      "type": "nominal"
    },
    "production_companies": {
      "type": "nominal"
    },
    "production_countries": {
      "type": "nominal"
    },
    "keywords": {
      "type": "nominal"
    }
  },
  "cleaning": [
    {
      "attribute": "title",
      "missing_strategy": "remove"
    },
    {
      "attribute": "release_date",
      "missing_strategy": "remove"
    },
    {
      "attribute": "vote_average",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "vote_count",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "revenue",
      "missing_strategy": "replace_missing_with_mean"
    }
  ],
  "transforming": [],
  "plotting": [
    {
      "action": "make_density_plot",
      "attribute": "revenue",
      "name": "RevenueDensityPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "revenue",
      "name": "RevenueHistogram",
      "kwargs": {
        "name": "Revenuse"
      }
    },
    {
      "action": "make_barplot",
      "attribute": "vote_average",
      "name": "VoteAverageHistogram",
      "kwargs": {
        "name": "VoteAverage"
      }
    },
    {
      "action": "make_boxplot",
      "attribute": "vote_average",
      "name": "VoteAverageHistogram",
      "kwargs": {
        "name": "VoteAverage"
      }
    },
    {
      "action": "make_barplot",
      "attribute": "vote_count",
      "name": "VoteCountHistogram",
      "kwargs": {
        "name": "VoteCount"
      }
    }
  ]
}
