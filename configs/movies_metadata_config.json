{
  "raw_dataset_path": "./data/raw/movies_metadata.parquet",
  "clean_dataset_path": "./data/transformed/movies_metadata.parquet",
  "preprocess_script_path": "preprocess_movies",
  "plot_directory_path": "plots",
  "attributes": {
    "id": {
      "type": "nominal"
    },
    "title": {
      "type": "nominal"
    },
    "vote_average": {
      "type": "real"
    },
    "vote_count": {
      "type": "real"
    },
    "status": {
      "type": "nominal"
    },
    "release_date": {
      "type": "nominal"
    },
    "revenue": {
      "type": "real"
    },
    "runtime": {
      "type": "real"
    },
    "adult": {
      "type": "nominal"
    },
    "budget": {
      "type": "real"
    },
    "popularity": {
      "type": "real"
    },
    "tagline": {
      "type": "nominal"
    },
    "keyword_1": {
      "type": "nominal"
    },
    "keyword_2": {
      "type": "nominal"
    },
    "keyword_3": {
      "type": "nominal"
    },
    "keyword_4": {
      "type": "nominal"
    },
    "genre_1": {
      "type": "nominal"
    },
    "genre_2": {
      "type": "nominal"
    },
    "genre_3": {
      "type": "nominal"
    },
    "genre_4": {
      "type": "nominal"
    },
    "production_company_1": {
      "type": "nominal"
    },
    "production_company_2": {
      "type": "nominal"
    },
    "production_company_3": {
      "type": "nominal"
    },
    "production_company_4": {
      "type": "nominal"
    },
    "production_country_1": {
      "type": "nominal"
    },
    "production_country_2": {
      "type": "nominal"
    },
    "production_country_3": {
      "type": "nominal"
    },
    "total_keywords": {
      "type": "real"
    },
    "total_genre": {
      "type": "real"
    },
    "total_production_company": {
      "type": "real"
    },
    "total_production_country": {
      "type": "real"
    }
  },
  "cleaning": [
    {
      "attribute": "title",
      "missing_strategy": "remove"
    }
  ],
  "transforming": [
    {
      "action": "make_mean_bins",
      "attribute": "vote_count",
      "args": ["freq", 7]
    },
    {
      "action": "make_mean_bins",
      "attribute": "vote_average",
      "args": ["freq", 7]
    },
    {
      "action": "make_mean_bins",
      "attribute": "revenue",
      "args": ["freq", 7]
    },
    {
      "action": "make_mean_bins",
      "attribute": "budget",
      "args": ["freq", 7]
    },
    {
      "action": "z_score_norm",
      "attribute": "popularity"
    }
  ],
  "plotting": [
    {
      "action": "make_word_cloud",
      "attribute": "title",
      "name": "MoviesMovieTitleWordCloud"
    },
    {
      "action": "make_density_plot",
      "attribute": "vote_average",
      "name": "MoviesVoteAverageDensity"
    },
    {
      "action": "make_boxplot",
      "attribute": "vote_average",
      "name": "MoviesVoteBoxPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "vote_count",
      "name": "MoviesVoteCountDensityPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "status",
      "name": "MoviesStatusHistogram",
      "kwargs": {
        "name": "Status"
      }
    },
    {
      "action": "make_word_cloud",
      "attribute": "status",
      "name": "MoviesStatusWordCloud"
    },
    {
      "action": "make_density_plot",
      "attribute": "revenue",
      "name": "MoviesRevenueDensityPlot"
    },
    {
      "action": "make_boxplot",
      "attribute": "revenue",
      "name": "MoviesRevenueBoxPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "runtime",
      "name": "MoviesRuntimeDensityPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "adult",
      "name": "MoviesAdultHistogram",
      "kwargs": {
        "name": "Adult"
      }
    },
    {
      "action": "make_density_plot",
      "attribute": "budget",
      "name": "MoviesBudgetDensityPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "popularity",
      "name": "MoviesPopularityDensityPlot"
    },
    {
      "action": "make_boxplot",
      "attribute": "popularity",
      "name": "MoviesPopularityBoxPlot"
    }
  ]
}
