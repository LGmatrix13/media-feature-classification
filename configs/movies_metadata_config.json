{
  "raw_dataset_path": "./data/raw/movies_metadata.parquet",
  "clean_dataset_path": "./data/cleaned/movies_metadata.parquet",
  "plot_directory_path": "plots",
  "attributes": {
    "title": {
      "type": "nominal"
    },
    "vote_average": {
      "type": "real"
    },
    "vote_count": {
      "type": "real"
    },
    "status": {
      "type": "nominal"
    },
    "release_date": {
      "type": "nominal"
    },
    "revenue": {
      "type": "real"
    },
    "runtime": {
      "type": "real"
    },
    "adult": {
      "type": "nominal"
    },
    "budget": {
      "type": "real"
    },
    "popularity": {
      "type": "real"
    },
    "genres": {
      "type": "nominal"
    },
    "production_companies": {
      "type": "nominal"
    },
    "production_countries": {
      "type": "nominal"
    },
    "keywords": {
      "type": "nominal"
    }
  },
  "cleaning": [
    {
      "attribute": "title",
      "missing_strategy": "remove"
    },
    {
      "attribute": "vote_average",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "vote_count",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "revenue",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "runtime",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "budget",
      "missing_strategy": "replace_missing_with_mean"
    },
    {
      "attribute": "popularity",
      "missing_strategy": "replace_missing_with_mean"
    }
  ],
  "transforming": [
    {
      "action": "make_mean_bins",
      "attribute": "vote_count",
      "args": ["freq", 5]
    },
    {
      "action": "make_mean_bins",
      "attribute": "vote_average",
      "args": ["freq", 5]
    },
    {
      "action": "z_score_norm",
      "attribute": "revenue",
      "args": ["freq", 5]
    },
    {
      "action": "z_score_norm",
      "attribute": "budget"
    },
    {
      "action": "make_mean_bins",
      "attribute": "popularity",
      "args": ["freq", 5]
    }
  ],
  "plotting": [
    {
      "action": "make_word_cloud",
      "attribute": "title",
      "name": "MoviesMovieTitleWordCloud"
    },
    {
      "action": "make_density_plot",
      "attribute": "vote_average",
      "name": "MoviesVoteAverageDensity"
    },
    {
      "action": "make_boxplot",
      "attribute": "vote_average",
      "name": "MoviesVoteBoxPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "vote_count",
      "name": "MoviesVoteCountDensityPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "status",
      "name": "MoviesStatusHistogram",
      "kwargs": {
        "name": "Status"
      }
    },
    {
      "action": "make_word_cloud",
      "attribute": "status",
      "name": "MoviesStatusWordCloud"
    },
    {
      "action": "make_density_plot",
      "attribute": "revenue",
      "name": "MoviesRevenueDensityPlot"
    },
    {
      "action": "make_boxplot",
      "attribute": "revenue",
      "name": "MoviesRevenueBoxPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "runtime",
      "name": "MoviesRuntimeDensityPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "adult",
      "name": "MoviesAdultHistogram",
      "kwargs": {
        "name": "Adult"
      }
    },
    {
      "action": "make_density_plot",
      "attribute": "budget",
      "name": "MoviesBudgetDensityPlot"
    },
    {
      "action": "make_density_plot",
      "attribute": "popularity",
      "name": "MoviesPopularityDensityPlot"
    },
    {
      "action": "make_boxplot",
      "attribute": "popularity",
      "name": "MoviesPopularityBoxPlot"
    }
  ]
}
