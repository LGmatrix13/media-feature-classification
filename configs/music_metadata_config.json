{
  "raw_dataset_path": "./data/raw/music_metadata.parquet",
  "clean_dataset_path": "./data/cleaned/music_metadata.parquet",
  "plot_directory_path": "plots",
  "attributes": {
    "title": {
      "type": "nominal"
    },
    "tag": {
      "type": "nominal"
    },
    "artist": {
      "type": "nominal"
    },
    "year": {
      "type": "real"
    },
    "views": {
      "type": "real"
    },
    "features": {
      "type": "nominal"
    }
  },
  "cleaning": [
    {
      "attribute": "id",
      "missing_strategy": "remove"
    },
    {
      "attribute": "title",
      "missing_strategy": "remove"
    },
    {
      "attribute": "tag",
      "missing_strategy": "remove"
    },
    {
      "attribute": "year",
      "missing_strategy": "remove"
    },
    {
      "attribute": "views",
      "missing_strategy": "replace_missing_with_median"
    }
  ],
  "transforming": [
    {
      "action": "make_mean_bins",
      "attribute": "views",
      "args": ["freq", 10]
    }
  ],
  "plotting": [
    {
      "action": "make_density_plot",
      "attribute": "views",
      "name": "MusicViewsDensityPlot"
    },
    {
      "action": "make_boxplot",
      "attribute": "views",
      "name": "MusicViewsBoxPlot"
    },
    {
      "action": "make_barplot",
      "attribute": "tag",
      "name": "MusicTagHistogram",
      "kwargs": {
        "name": "Tags"
      }
    },
    {
      "action": "make_barplot",
      "attribute": "year",
      "name": "MusicYearHistorgram",
      "kwargs": {
        "name": "Year"
      }
    },
    {
      "action": "make_word_cloud",
      "attribute": "title",
      "name": "MusicTitleWordCloud"
    },
    {
      "action": "make_word_cloud",
      "attribute": "tag",
      "name": "MusicTagWordCloud"
    },
    {
      "action": "make_word_cloud",
      "attribute": "artist",
      "name": "MusicArtistWordCloud"
    }
  ]
}
